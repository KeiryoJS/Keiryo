{
  "name": "neocord",
  "version": "0.0.0-alpha.1",
  "description": "A powerful and feature-rich discord library.",
  "keywords": [
    "discord",
    "typescript",
    "powerful",
    "feature-rich"
  ],
  "author": "MeLike2D",
  "homepage": "https://github.com/neo-cord/neocord#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-cord/neocord.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --removeComments",
    "build:types": "tsc -d",
    "lint": "eslint . --ext .ts",
    "bundle:types": "dts-bundle --name neocord --main ./dist --removeSource --outputAsModuleFolder --baseDir ./dist --out ../typings/index.d.ts"
  },
  "dependencies": {
    "@neocord/gateway": "1.0.9",
    "@neocord/rest": "^1.0.1",
    "@neocord/utils": "^1.0.10",
    "discord-api-types": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "typescript": "^4.0.2"
  }
}
